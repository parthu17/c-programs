#include <stdio.h>
#include <math.h>
int main(){
int n,temp=0,a,b,c,rem,sum=0,i;
char ch;
printf("enter the number");
scanf("%d",&n);
a=n;
while(n!=0){
temp++;
n=n/10;
}
n=a;
while(a!=0){
rem=n%10;
sum=sum+ pow(rem,temp);
a=a/10;
}
if(sum==n){
printf("armstrong");
}
else{
printf("not armstrong");
}
sum=0;
for(i=1;i<n;;i++){
if(n%i==0){
sum+=i;
}
if(sum==n){
printf("strong");
}
else{
printf("not strong");
}
printf("enter the two numbers for cal");
scanf("%d%d",&a,&b);
printf("enter operation");
scanf("%c",&ch);
switch(ch){
case'+': printf("%d",a+b);
break;
case'*': printf("%d",a*b);
break;
case'/': printf("%f",(float)a/b);
break;
case'%': printf("%d",a%b);
break;
default: printf("worng");
}
printf("enter sides of triangle");
scanf("%d%d%d",&a,&b,&c);
switch(a==b &&b==c){
case 1:printf("equilateral";
break;
case 0: if(a==b||b==c||c==a){
         printf("isosceles");
         else{
         printf("scalene");
  }
  return 0;
  }
  

