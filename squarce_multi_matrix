# include <stdio.h>
# include <stdlib.h>
void squarce(int*,int,int);
int main(){
int r1,c1,r2,c2,i,j,k;
printf("enter number of rows and columns");
scanf("%d%d",&r1,&c1);
int *p[r1],a[r1][c1];
printf("enter the first matrix");
for(i=0;i<r1;i++){
 for (j=0;j<c1;j++){
  scanf("%d",&a[i][j]);
}
}
for(i=0;i<r1;i++){
 p[i] = &a[i][0];
}
printf("enter number of rows and columns");
scanf("%d%d",&r2,&c2);
int *q[r2],a[r2][c2];
printf("enter the first matrix");
for(i=0;i<r2;i++){
 for (j=0;j<c2;j++){
  scanf("%d",&b[i][j]);
}
}
for(i=0;i<r2;i++){
 q[i] = &bi][0];
}
if(c1==r2){
int c[r1][c2];
for(i=0;i<r1;i++){
for(j=0,j<c2;j++){
c[i][j]=0;
for(k=0;k<c1;k++){
c[i][j]+=*((p+i)+k) + *((q+k)+j);
}
}
}
for(i=0;i<r1;i++){
for(j=0,j<c2;j++){
printf("%d",&c[i][j]);
}}
squarce(p,r1,c1);
squarce(q,r2,c2);
return 0;
}
void squarce(int *p,r1,c1){
int count=0,i,j;
for(i=0;i<r1,i++){
for(j=0;j<c1;j++){
if(*((p+i)+j)==0){
count++;
}
}
if(count>((r1*c1)/2)){
printf("squarce");
else{
printf("not squarce");
}
}





