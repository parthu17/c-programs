#include <stdio.h>
#include <stdlib.h>
void christree();
void rightpat();
int main(){
int r,c,i,j;
printf("enter number of rows and columns");
scanf("%d%d",&r,&c);
int *p[r];
for(i=0;i<r;i++){
p[i] = int* malloc(c* sizeof(int));
}
printf("enter elements");
for(i=0;i<r;i++){
for(j=0;j<c;j++){
scanf("%d",(p+i)+j);
}
}
printf("transpose:");
for(i=0;i<r;i++){
for(j=0;j<c;j++){
printf("%d",*(p+j)+i);
}
}
christree();
rightpat();
return 0;
}
void christree(){
int temp1=2,temp2=1,i,j;
for(i=1,i<=3,i++){
for(j=1;j<=temp1;j++){
printf(" ");
}
for(j=1;j<=temp2;j++){
printf("*");
}
temp--;
temp2+=2;
}
}
void rightpat(){
int i,j;
for(i=1;i<=4;i++){
for(j=1;j<=4-i;j++){
printf(" ");
}
for(j=1;j<=i;j++){
printf("*");
}
}}




